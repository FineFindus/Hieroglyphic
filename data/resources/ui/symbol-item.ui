<?xml version="1.0" encoding="UTF-8"?>
<interface>
    <template class="SymbolItem" parent="GtkBox">
        <property name="orientation">horizontal</property>
        <child>
            <object class="GtkImage">
                <property name="icon-name" bind-source="SymbolItem" bind-property="icon" bind-flags="sync-create"/>
                <property name="icon-size">GTK_ICON_SIZE_LARGE</property>
                <property name="margin-start">8</property>
                <property name="margin-end">16</property>
                <!-- <property name="icon-name">penguin-symbolic</property> -->
            </object>
        </child>
        <child>
            <object class="GtkBox">
                <property name="orientation">vertical</property>
                <child>
                    <object class="GtkLabel">
                        <!-- <property name="label">\usepackage{ amssymb }</property> -->
                        <property name="halign">GTK_ALIGN_START</property>
                        <property name="label" bind-source="SymbolItem" bind-property="package" bind-flags="sync-create"/>
                    </object>
                </child>
                <child>
                    <object class="GtkLabel">
                        <property name="halign">GTK_ALIGN_START</property>
                        <!-- <property name="label">\varpropto</property> -->
                        <property name="label" bind-source="SymbolItem" bind-property="command" bind-flags="sync-create"/>
                        <style>
                            <class name="title-3"/>
                        </style>
                    </object>
                </child>
                <child>
                    <object class="GtkLabel">
                        <property name="halign">GTK_ALIGN_START</property>
                        <!-- <property name="label">mathmode (score 0.1473)</property> -->
                        <property name="label" bind-source="SymbolItem" bind-property="mode" bind-flags="sync-create"/>
                        <style>
                            <class name="caption"/>
                        </style>
                    </object>
                </child>
            </object>
        </child>
    </template>
</interface>
