<?xml version="1.0" encoding="UTF-8"?>
<component type="desktop-application">
  <id>@app-id@</id>
  <metadata_license>CC0</metadata_license>
  <project_license>MIT</project_license>
  <name>TeX Match</name>
  <summary>Find LaTeX symbols by sketching</summary>
  <!-- developer_name tag deprecated with Appstream 1.0 -->
  <developer_name translatable="no">FineFindus</developer_name>
  <developer id="github.com">
      <name translatable="no">FineFindus</name>
  </developer>
  <update_contact>finefindus@proton.me</update_contact>
  <project_group>GNOME</project_group>
  <description>
    <p>If you work with LaTeX, you know its difficult to memorize the names of all the symbols. TeX Match allows you to search through over 1000 different LaTeX symbols by sketching.</p>
  </description>
  <screenshots>
    <screenshot type="default">
        <image>https://raw.githubusercontent.com/FineFindus/TeX-Match/main/data/resources/screenshots/light.png</image>
      <caption>Main Window</caption>
    </screenshot>
    <screenshot type="default">
      <image>https://raw.githubusercontent.com/FineFindus/TeX-Match/main/data/resources/screenshots/dark.png</image>
      <caption>Main Window (Adwaita-Dark)</caption>
    </screenshot>
  </screenshots>
  <url type="homepage">https://github.com/FineFindus/TeX-Match</url>
  <url type="bugtracker">https://github.com/FineFindus/TeX-Match/issues</url>
  <url type="help">https://github.com/FineFindus/TeX-Match/issues</url>
  <url type="vcs-browser">https://github.com/FineFindus/TeX-Match</url>
  <url type="contribute">https://github.com/FineFindus/TeX-Match#contributing</url>
  <url type="translate">https://github.com/FineFindus/TeX-Match#translation</url>
  <content_rating type="oars-1.0" />
  <releases>
    <release version="1.0.0" date="2024-03-01">
      <description>
        <p>Inital release</p>
      </description>
    </release>
  </releases>
  <kudos>
    <!--
    GNOME Software kudos:
    https://gitlab.gnome.org/GNOME/gnome-software/blob/master/doc/kudos.md
    -->
    <kudo>ModernToolkit</kudo>
    <kudo>HiDpiIcon</kudo>
  </kudos>
  <translation type="gettext">@gettext-package@</translation>
  <launchable type="desktop-id">@app-id@.desktop</launchable>

  <custom>
    <value key="GnomeSoftware::key-colors">[(120, 217, 247)]</value>
  </custom>
</component>
